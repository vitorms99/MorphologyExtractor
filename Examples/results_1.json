{
    "initial_settings": {
        "cutout_folder": "/home/vitorms/Dropbox/galmex/Examples/test_galaxies",
        "output_file": "results_1.csv",
        "output_folder": "/home/vitorms/Dropbox/galmex/Examples",
        "cores": 1,
        "hdu": 0,
        "estimate_uncertainty": false,
        "nsim": 10,
        "gain": 1,
        "exptime": 1
    },
    "preprocessing": {
        "background": {
            "method": "frame",
            "flat": {
                "median": 0.0,
                "std": 1.0
            },
            "frame": {
                "fraction": 0.1,
                "sigma_clip": true,
                "sigma_thresh": 3.0
            },
            "sep": {
                "bw": 32,
                "bh": 32,
                "fw": 32,
                "fh": 32
            },
            "load": {
                "folder": "/home/vitorms/Dropbox/galmex",
                "prefix": "",
                "suffix": "",
                "hdu": 0
            }
        },
        "detection": {
            "method": "SEP",
            "sextractor": {
                "folder": "/home/vitorms/Dropbox/galmex",
                "config": "default.sex"
            },
            "sep": {
                "threshold": 1,
                "minarea": 10,
                "filter_type": "matched",
                "deblend_nthresh": 32,
                "deblend_cont": 0.001
            }
        },
        "cleaning": {
            "method": "isophotes"
        },
        "profile": {
            "aperture_type": "elliptical",
            "eta": 0.2,
            "optimize": true,
            "step": 0.05
        },
        "flagging": {
            "k_flag": 1.5,
            "max_nsec": 4,
            "min_dmag": 1.0
        }
    },
    "CAS": {
        "measure_c": true,
        "measure_a": true,
        "measure_s": true,
        "concentration": {
            "aperture": "elliptical",
            "f_inner": 0.2,
            "f_outer": 0.8,
            "k_max": 1.5,
            "smooth": "no",
            "smooth_factor": 5.0
        },
        "asymmetry": {
            "segmentation": "circle",
            "k": 1.5,
            "rotation": 180.0,
            "formula": "pixel-wise",
            "smooth": "yes",
            "smooth_factor": 0.17,
            "remove_center": "no",
            "remove_percentage": 5.0
        },
        "smoothness": {
            "segmentation": "circle",
            "k": 1.5,
            "formula": "pixel-wise",
            "filter": "box",
            "smooth_factor": 0.3,
            "remove_center": "yes",
            "remove_percentage": 0.15
        }
    },
    "MEGG": {
        "measure_m20": true,
        "measure_e": true,
        "measure_gini": true,
        "measure_g2": true,
        "m20": {
            "segmentation": "intensity",
            "k": 1.0,
            "fraction": 0.2,
            "smooth": "no",
            "smooth_factor": 0.2,
            "remove_center": "no",
            "remove_percentage": 5.0
        },
        "entropy": {
            "segmentation": "intensity",
            "k": 1.0,
            "bins_method": "auto",
            "nbins": 32,
            "normalize": true,
            "smooth": "no",
            "smooth_factor": 0.2,
            "remove_center": "no",
            "remove_percentage": 5.0
        },
        "gini": {
            "segmentation": "intensity",
            "k": 1.0,
            "smooth": "no",
            "smooth_factor": 0.2,
            "remove_center": "no",
            "remove_percentage": 5.0
        },
        "g2": {
            "segmentation": "intensity",
            "k": 1.0,
            "module_tol": 0.06,
            "phase_tol": 130.0,
            "smooth": "no",
            "smooth_factor": 0.2
        }
    }
}